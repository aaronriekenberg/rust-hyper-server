{
    "context_configuration": {
        "context": "/cgi-bin"
    },
    "server_configuration": {
        "server_protocol": "HTTP1",
        "bind_address": "/var/www/run/cgi/socket"
    },
    "command_configuration": {
        "max_concurrent_commands": 10,
        "semaphore_acquire_timeout": "200ms",
        "commands": [
            {
                "id": "chronyc_sources",
                "description": "chronyc sources",
                "command": "/usr/bin/chronyc",
                "args": [
                    "-n",
                    "sources",
                    "-v"
                ]
            },
            {
                "id": "df",
                "description": "df",
                "command": "/usr/bin/df",
                "args": [
                    "-h"
                ]
            },
            {
                "id": "git_log",
                "description": "git log -1",
                "command": "/usr/bin/git",
                "args": [
                    "log",
                    "-1"
                ]
            },
            {
                "id": "ip_addr",
                "description": "ip addr",
                "command": "/usr/sbin/ip",
                "args": [
                    "addr"
                ]
            },
            {
                "id": "lscpu",
                "description": "lscpu",
                "command": "/usr/bin/lscpu"
            },
            {
                "id": "netstat_an",
                "description": "netstat -an",
                "command": "/usr/bin/netstat",
                "args": [
                    "-a",
                    "-n"
                ]
            },
            {
                "id": "pitemp",
                "description": "pitemp",
                "command": "/home/aaron/repo/raspberrypi/pitemp.sh"
            },
            {
                "id": "pithrottle",
                "description": "pithrottle",
                "command": "/home/aaron/repo/raspberrypi/pithrottle.sh"
            },
            {
                "id": "top",
                "description": "top",
                "command": "/usr/bin/top",
                "args": [
                    "-b",
                    "-n1"
                ]
            },
            {
                "id": "top_ores",
                "description": "top -o RES",
                "command": "/usr/bin/top",
                "args": [
                    "-b",
                    "-n1",
                    "-o",
                    "RES"
                ]
            },
            {
                "id": "uptime",
                "description": "uptime",
                "command": "/usr/bin/uptime"
            },
            {
                "id": "vmstat",
                "description": "vmstat",
                "command": "/usr/bin/vmstat"
            },
            {
                "id": "w",
                "description": "w",
                "command": "/usr/bin/w"
            }
        ]
    },
    "static_file_configuration": {
        "path": "/var/www/html/aaronr.digital"
    }
}